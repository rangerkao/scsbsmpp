    <servlet-class>com.scsb.servlet.ManagerPersonalServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>managerPersonalServlet</servlet-name>
    <url-pattern>/managerPersonal.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>editPasswordServlet</servlet-name>
    <servlet-class>com.scsb.servlet.EditPasswordServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>editPasswordServlet</servlet-name>
    <url-pattern>/editPassword.do</url-pattern>
  </servlet-mapping>
  <filter>
